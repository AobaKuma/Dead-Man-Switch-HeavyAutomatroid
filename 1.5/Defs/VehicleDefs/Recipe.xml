<Defs>
  <RecipeDef MayRequire="rimthunder.core">
    <defName>DMS_Make_Mule_ATV_Blueprint</defName>
    <label>mule ATV</label>		
    <description>assemble a vehicle.</description>
		<jobString>vehicle assembling.</jobString>
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<effectWorking>Repair</effectWorking>
		<soundWorking>Recipe_Machining</soundWorking>
		<unfinishedThingDef>RTC_UnfinishedVehicle</unfinishedThingDef>
    <researchPrerequisite>Machining</researchPrerequisite>
		<recipeUsers>
			<li>RT_AssemblyCrane</li>
		</recipeUsers>
		<workSkill>Crafting</workSkill>
    <workAmount>2000</workAmount>
    <!--20000 -->
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>EngineLight</li>
          </thingDefs>
        </filter>
        <count>2</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>ComponentIndustrial</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>100</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Chemfuel</li>
          </thingDefs>
        </filter>
        <count>120</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>EngineLight</li>
        <li>Chemfuel</li>
        <li>ComponentIndustrial</li>
        <li>Steel</li>
      </thingDefs>
    </fixedIngredientFilter>
    <modExtensions>
      <li Class="Motorization.ModExt_RTCVehicleRecipe">
        <thing>DMS_Mule_ATV</thing>
      </li>
    </modExtensions>
  </RecipeDef>

