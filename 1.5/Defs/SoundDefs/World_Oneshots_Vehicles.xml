  <!-- 通用 -->
  <SoundDef>
    <defName>DMS_VehicleEnter</defName>  
    <context>MapOnly</context>  
    <maxSimultaneous>1</maxSimultaneous>  
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Clip">
            <clipPath>DMS_Vehicle/Vehicle_Enter</clipPath>
